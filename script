local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/BlizTBr/scripts/main/Orion%20X')))()

local Window = OrionLib:MakeWindow({Name = "CAT HUB", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local t_Tab = Window:MakeTab({
    Name = "teleport",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

t_Tab:AddButton({
    Name = "Home1",
    Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-500.258, -5, -155.762)
    end
})

t_Tab:AddButton({
    Name = "Home2",
    Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-552.647,-5,80.451)
    end
})

t_Tab:AddButton({
    Name = "Home3",
    Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(278.434,-5,460.492)
    end
})

t_Tab:AddButton({
    Name = "Home4",
    Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(505.87,83,-355.719)
    end
})

t_Tab:AddButton({
    Name = "Home5",
    Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(554.032,123,-93.112)
    end
})

t_Tab:AddButton({
    Name = "空島",
    Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(138.469,346,352.83)
    end
})

local AntiGrabToggle = false
local Event;

t_Tab:AddToggle({
    Name = "Anti-Grab",
    Callback = function(val)
        AntiGrabToggle = val
        local BeingHeld = game.Players.LocalPlayer.Character.HumanoidRootPart.BeingHeld
        local Player = game.Players.LocalPlayer
        local R = game:GetService("RunService")
        local StruggleEvent = game.ReplicatedStorage.StruggleEvent

        if AntiGrabToggle then
            Event = R.RenderStepped:Connect(function()
                if BeingHeld.Value then
                    Player.Character.HumanoidRootPart.Velocity = Vector3.new()
                    Player.Character.HumanoidRootPart.Anchored = true
                    StruggleEvent:FireServer(Player)
                else
                    Player.Character.HumanoidRootPart.Velocity = Vector3.new()
                    Player.Character.HumanoidRootPart.Anchored = false
                end
            end)
        else
            if Event then
                Event:Disconnect()
                Event = nil
                Player.Character.HumanoidRootPart.Velocity = Vector3.new()
                Player.Character.HumanoidRootPart.Anchored = false
            end
        end
    end
})
