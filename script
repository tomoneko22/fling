-- Orion Xライブラリをロード
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/BlizTBr/scripts/main/Orion%20X')))()

-- ウィンドウを作成
local Window = OrionLib:MakeWindow({Name = "CAT HUB", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

-- タブを作成
local t_Tab = Window:MakeTab({
    Name = "teleport",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

-- ボタンを追加
t_Tab:AddButton({
    Name = "Home1",
    Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-500.258, -5, -155.762)
    end
})

t_Tab:AddButton({
    Name = "Home2",
    Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-552.647,-5,80.451)
    end
})

t_Tab:AddButton({
    Name = "Home3",
    Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(278.434,-5,460.492)
    end
})

t_Tab:AddButton({
    Name = "Home4",
    Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(505.87,83,-355.719)
    end
})

t_Tab:AddButton({
    Name = "Home5",
    Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(554.032,123,-93.112)
    end
})

t_Tab:AddButton({
    Name = "空島",
    Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(138.469,346,352.83)
    end
})

-- AntiGrabトグルを追加
t_Tab:AddToggle({
	Name = "AntiGrab",
	Default = false,
	Callback = function(Value)
		_G.AntiGrab = Value
	end    
})

-- BeingHeldの値が変更されたときに実行される関数を定義
BeingHeld.Changed:Connect(function(C)
		if _G.AutoAttacker and not _G.AntiGrab then
			-- 他のプレイヤーに掴まれたときの処理（省略）
		end

		-- Cがtrueで、PlayerWhoIsHoldingが開発者でない場合
		if C == true and not verifyDev(PS:FindFirstChild(PlayerWhoIsHolding)) then
			local char = Player.Character or Player.CharacterAdded:Wait()
			local hrp = char:WaitForChild("HumanoidRootPart")

			-- BeingHeld.Valueがtrueの場合
			if BeingHeld.Value then
				local Event;
				
				Event = R.RenderStepped:Connect(function()
					-- BeingHeld.Valueと_G.AntiGrabがtrueの場合
					if BeingHeld.Value and _G.AntiGrab then
						hrp.Velocity = Vector3.new()
						hrp.Anchored = true
						StruggleEvent:FireServer(Player)
					else
						hrp.Velocity = Vector3.new()
						hrp.Anchored = false
						Event:Disconnect()
					end
				end)
			end
		end
	end)
